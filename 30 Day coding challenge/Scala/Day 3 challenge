object Solution {

    def main(args: Array[String]) {
        val sc = new java.util.Scanner (System.in);
        var N = sc.nextInt();
        if (N%2 != 0) println("Weird") else {if ((N%2 == 0) & ((N>=2) & (N<=5))) println("Not Weird") 
            else {if ((N%2 == 0) & ((N>=6) & (N<=20))) println("Weird") else {if ((N%2 == 0) & (N>20)) println("Not Weird")}} 
                  
                     
        }
    }
}
